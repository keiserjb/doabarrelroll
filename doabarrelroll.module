<?php

/**
 * @file
 * Adds the Barrel Roll feature to search forms.
 */

use Drupal\Core\Form\FormStateInterface;

/**
 * Implements hook_form_alter().
 */
function doabarrelroll_form_alter(&$form, FormStateInterface $form_state, $form_id)
{
  if ($form_id == 'search_block_form' || $form_id == 'search_form') {

    $form['#attached']['library'][] =    'doabarrelroll/doabarrelroll';


  }
}
